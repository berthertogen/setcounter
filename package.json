{
  "name": "setcounter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-and-wait": "npm run start & wait-on http://localhost:4200",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "ng lint --fix",
    "prettier": "prettier --write",
    "e2e": "ng run setcounter:cypress-open",
    "postinstall": "husky install && husky add .husky/pre-commit \"npx lint-staged\""
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@ngrx/component-store": "^12.1.0",
    "luxon": "^1.27.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@briebug/cypress-schematic": "^5.0.0",
    "@testing-library/angular": "^10.8.2",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/luxon": "^1.26.5",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "cypress": "^7.4.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "karma-coverage": "~2.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "prettier-eslint": "^12.0.0",
    "ts-autofixture": "^1.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4",
    "wait-on": "^6.0.0"
  }
}
